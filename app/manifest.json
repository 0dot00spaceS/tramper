{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Tamper Chrome (application)",
  "description": "Intercept and modify Chrome requests",
  "version": "1.3.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": { "16": "tamper-16.png", "48": "tamper-48.png", "128": "tamper-128.png" },
  "permissions": [
    {"socket":["tcp-connect", "tcp-listen"]},
    "clipboardWrite"
  ],
  "key": ""
}
